<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Reporte de congestiones</title>

  <script src="js/redireccion.js"></script>
  <link rel="stylesheet" href="css/styleCentral.css">
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      border: 1px solid #000;
      padding: 8px;
      text-align: center;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h2>Reporte Material</h2>
  <table>
    <thead>
      <tr>
        <th>NÃºmero de Viaje</th>
        <th>Material</th>
        <th>Cantidad</th>
        <th>Fecha</th>
      </tr>
    </thead>
    <tbody id="tablaRegistros">
      <% registros.forEach(registro => { %>
        <tr>
          <td><%= registro.numeroViaje %></td>
          <td><%= registro.tipoMaterial %></td>
          <td><%= registro.peso %></td>
          <td><%= registro.fecha.toLocaleDateString() %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  <button onclick="redireccionGerente()">Atras</button>
</body>
</html> 